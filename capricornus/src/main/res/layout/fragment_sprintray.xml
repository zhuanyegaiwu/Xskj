<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:layout_marginLeft="61dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:gravity="center"
        style="@style/TextCommStyleBlack"
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reservoir 1 \n fullnes"
       />
    <TextView
        android:layout_marginLeft="247dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:gravity="center"
        style="@style/TextCommStyleBlack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reservoir 2 \n fullnes"
        />

    <ImageView
        android:background="@color/color_C4C4C4"
        android:id="@+id/imageView2"
        android:layout_width="93dp"
        android:layout_height="130dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="22dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <ImageView
        android:background="@color/color_C4C4C4"
        android:id="@+id/imageView3"
        android:layout_width="93dp"
        android:layout_height="130dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="22dp"
        app:layout_constraintStart_toEndOf="@id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />


    <fragment
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/imageView3"
        app:navGraph="@navigation/sprint_ray_navigation"
        android:id="@+id/sprintray_nav_host_fragment"
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:layout_width="436dp"
        android:layout_height="match_parent">

    </fragment>
</androidx.constraintlayout.widget.ConstraintLayout>