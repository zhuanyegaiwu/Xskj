<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/ivOk"
        android:layout_marginLeft="250dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@mipmap/icon_waring"
        android:layout_width="64dp"
        android:layout_height="45dp">

    </ImageView>



    <TextView
        android:id="@+id/textView1"
        android:layout_marginLeft="143dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toBottomOf="@id/ivOk"
        android:text="You might not be getting optimal results."
        style="@style/TextCommStyleBlack"
        android:layout_width="298dp"
        android:layout_height="wrap_content">

    </TextView>

    <TextView
        android:id="@+id/textView2"
        android:layout_marginLeft="143dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@id/textView1"
        android:text="Please replace your IPA liqid to get optimal results."
        style="@style/TextCommStyleBlack"
        android:layout_width="298dp"
        android:layout_height="76dp">

    </TextView>


    <TextView
        android:id="@+id/tvStartWash"
        android:layout_marginLeft="23dp"
        app:layout_constraintStart_toStartOf="@id/textView2"
        android:layout_marginTop="165dp"
        app:layout_constraintTop_toBottomOf="@id/ivOk"
        android:background="@color/color_000000"
        android:text="Start Wash"
        android:gravity="center"
        style="@style/TextCommStyleWhite"
        android:layout_width="200dp"
        android:layout_height="48dp">

    </TextView>

</androidx.constraintlayout.widget.ConstraintLayout>